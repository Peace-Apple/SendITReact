{
  "name": "senditreact",
  "version": "1.0.0",
  "description": "My sendit react app",
  "main": "src/index.js",
  "engines": {
    "node": "10.15.1"
  },
  "scripts": {
    "test": "jest -u --coverage",
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "travis": "CI=true react-scripts test --env=jsdom -u  --colors",
    "coverage": "react-scripts test --env=jsdom -u --colors --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Peace-Apple/SendITReact.git"
  },
  "author": "Apple Lurve",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Peace-Apple/SendITReact/issues"
  },
  "homepage": "https://github.com/Peace-Apple/SendITReact#readme",
  "dependencies": {
    "express": "^4.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/setupTests.js",
      "!src/index.js",
      "!src/store/index.js"
    ]
  }
}
